# Copy this file to .env and fill in your values
# NEVER commit .env to git!

# AWS Credentials (use session tokens when possible)
AWS_ACCESS_KEY_ID=your-access-key-here
AWS_SECRET_ACCESS_KEY=your-secret-key-here
AWS_SESSION_TOKEN=your-session-token-here

# AWS Account IDs
AWS_SANDBOX_ACCOUNT_ID=123456789012
AWS_NONPROD_ACCOUNT_ID=234567890123

# Optional: AWS Profile (instead of credentials)
# AWS_PROFILE=your-profile-name